{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server.js"
      }
    ],
    
    "ignoreCommand": "echo 'Ignoring unnecessary files...'",
    "functions": {
      "api/**/*.js": {
        "memory": 512,
        "maxDuration": 10
      }
    }
  }
  